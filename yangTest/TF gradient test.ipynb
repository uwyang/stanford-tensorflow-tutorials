    "x = tf.Variable(2.0)\n",
    "y = 2.0*(x**3)\n",
    "z = 3.0 + y**2\n",
    "\n",
    "grad_z = tf.gradients(z, [x, y])\n",
    "with tf.Session() as sess: \n",
    "    sess.run(x.initializer)\n",
    "    print(sess.run(grad_z))"
